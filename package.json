{
  "name": "futility",
  "version": "1.0.0-alpha",
  "main": "index.js",
  "repository": "git@github.com:SMontgomery/Futility.git",
  "author": "Sean Montgomery <sean@montry.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf dist && shx mkdir dist",
    "lint": "eslint 'src/**/*.js'",
    "lint-fix": "eslint --fix --quiet 'src/**/*.js'",
    "start": "yarn run clean && parcel src/index.html",
    "prebuild": "node prebuild.js",
    "build": "yarn run clean && parcel build src/index.html",
    "pretest": "eslint 'src/**/*.js'",
    "test": "jest",
    "analyze": "yarn run build && source-map-explorer 'prod/*.js'",
    "licenseSummary": "license-checker --production --summary"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "debug": "^4.1.1",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "rc-slider": "^8.6.9",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-color": "^2.17.0",
    "react-dom": "^16.8.6",
    "react-localize-redux": "^3.5.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "deasync": "^0.1.14",
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.5.0",
    "license-checker": "^25.0.1",
    "npm-license-crawler": "^0.2.1",
    "parcel-bundler": "^1.12.0",
    "sass": "^1.17.2",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "source-map-explorer": "^1.8.0"
  }
